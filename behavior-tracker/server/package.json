{
  "name": "behavior-tracker-server",
  "version": "1.0.0",
  "description": "HIPAA-compliant behavior tracking API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "db:migrate": "psql $DATABASE_URL -f src/db/schema.sql",
    "db:seed": "tsx src/db/seed.ts"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.1",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.11.10",
    "eslint": "^9.15.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^2.1.8"
  }
}
